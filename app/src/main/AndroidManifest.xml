<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alexsalupa97.bloodbank">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_bloodbank_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_bloodbank_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitati.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitati.SigninActivity"
            android:label="Sign In"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".Activitati.SignupActivity"
            android:label="Sign Up" />
        <activity android:name=".Activitati.AlegereLoginActivity" />
        <activity
            android:name=".Activitati.PrimaPaginaActivity"
            android:label="Pagina Principala" />
        <activity android:name=".Activitati.IntrebariActivity" />
        <activity android:name=".Activitati.FinalizareSuccesIntrebariActivity" />
        <activity android:name=".Activitati.FinalizareFailIntrebariActivity" />
        <activity android:name=".Activitati.CompatibilitatiActivity"></activity>
    </application>

</manifest>